<h1>Listing degrees</h1>

<table>
  <thead>
    <tr>
      <th>Semester of deregistration</th>
      <th>Grade</th>
      <th>Number of semesters</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @degrees.each do |degree| %>
      <tr>
        <td><%= degree.semester_of_deregistration %></td>
        <td><%= degree.grade %></td>
        <td><%= degree.number_of_semesters %></td>
        <td><%= link_to 'Show', degree %></td>
        <td><%= link_to 'Edit', edit_degree_path(degree) %></td>
        <td><%= link_to 'Destroy', degree, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Degree', new_degree_path %>
